<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.ui.actionSets">
      <actionSet
            label="Versoin Checker Action Set"
            visible="true"
            id="VersionChecker.actionSet">
         <menu
               label="Version &amp;Checker"
               id="sampleMenu">
            <separator
                  name="sampleGroup">
            </separator>
         </menu>
         <action
               label="&amp;Version Checker"
